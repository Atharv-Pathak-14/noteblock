{
  "name": "noteblock",
  "version": "2.0.0",
  "description": "An open-source music bot based on lavalink.",
  "main": "src/main",
  "repository": "https://github.com/KagChi/noteblock.git",
  "author": "KagChi",
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "@kirishima/core": "^0.5.0",
    "@kirishima/queue": "^0.3.3",
    "@kirishima/rest": "^0.1.3",
    "@sapphire/decorators": "^4.3.1",
    "@sapphire/framework": "^3.0.0-next.5524516.0",
    "@sapphire/plugin-api": "^3.2.0",
    "discord-api-types": "^0.29.0",
    "discord.js": "^13.6.0",
    "lavalink-api-types": "^0.1.9"
  },
  "engines": {
    "node": ">=v16.x"
  },
  "scripts": {
    "start": "npm run build && node -r dotenv/config dist",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "lint": "eslint src/**/**.ts",
    "lint:fix": "eslint src/**/**.ts --fix"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "2.25.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  },
  "eslintConfig": {
    "extends": "google",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "env": {
      "es2021": true,
      "node": true
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "new-cap": "off",
      "require-jsdoc": "off",
      "max-len": "off",
      "require-await": "error"
    }
  }
}
